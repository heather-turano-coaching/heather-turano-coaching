{
  "name": "@heather-turano-coaching/heather-turano-coaching",
  "version": "1.0.0",
  "description": "All applications and source code for https://livelifemindful.com",
  "main": "index.js",
  "scripts": {
    "clean": "lerna clean --yes && lerna exec -- rimraf lib/ dist/",
    "bootstrap": "lerna bootstrap --hoist && rimraf **/@types/react-native",
    "bootstrap:ci": "lerna bootstrap --hoist",
    "version": "lerna version --yes",
    "reset": "npm run clean && npm run bootstrap",
    "release": "npm run build && lerna publish --yes",
    "compile": "lerna run compile",
    "check": "lerna exec -- ncu",
    "update": "npm run check -- -u && npm run bootstrap",
    "test": "jest",
    "lint:fix": "eslint './packages/**/src/**/*.js' --fix",
    "build:all": "npm run build:ds && npm run build:components && npm run build:blog && npm run build:serverless",
    "build:components": "npm run build:hooks && lerna run build --scope=@heather-turano-coaching/components  --stream",
    "build:components:storybook": "npm run build:ds && && npm run build:hooks && lerna run build:storybook --scope=@heather-turano-coaching/components --stream",
    "build:ds": "lerna run build --scope=@heather-turano-coaching/design-system --stream",
    "build:hooks": "lerna run build --scope=@heather-turano-coaching/hooks --stream",
    "build:blog": "npm run build:ds && npm run build:components && lerna run build --scope=@live-life-mindful/blog --stream",
    "build:serverless": "lerna run build --scope=@heather-turano-coaching/serverless --stream",
    "publish:serverless:dev": "lerna run publish:dev --scope=@heather-turano-coaching/serverless --stream",
    "publish:serverless:prod": "lerna run publish:prod --scope=@heather-turano-coaching/serverless --stream",
    "start:ds": "lerna run start --scope=@heather-turano-coaching/design-system --stream",
    "start:components": "npm run build:ds && npm run build:hooks && cd ./packages/components && npm run start",
    "start:components:storybook": "npm run build:ds && npm run build:hooks && cd ./packages/components && npm run buildDevUtils && npm run start:storybook",
    "start:blog": "cd packages/blog && npm run start",
    "start:serverless": "cd packages/serverless && npm run start",
    "start:hooks": "cd packages/hooks && npm run start",
    "preview:blog": "cd packages/blog && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:imaginedelements/heather-turano-coaching/live-life-mindful.git"
  },
  "keywords": [
    "live-life-mindful",
    "live",
    "life",
    "mindful",
    "heather-turano-coaching",
    "heather",
    "turano"
  ],
  "author": "Drew DeCarme (drew@imaginedelements.com)",
  "license": "ISC",
  "dependencies": {
    "@heather-turano-coaching/components": "file:packages/components",
    "@heather-turano-coaching/design-system": "file:packages/design-system",
    "@heather-turano-coaching/hooks": "file:packages/hooks",
    "@heather-turano-coaching/prettier-config": "file:packages/prettier",
    "@heather-turano-coaching/serverless": "file:packages/serverless",
    "@heather-turano-coaching/typescript": "file:packages/typescript",
    "@live-life-mindful/blog": "file:packages/blog",
    "gatsby-plugin-ghost-manifest": "file:packages/blog/plugins/gatsby-plugin-ghost-manifest"
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.8.3",
    "@babel/runtime-corejs2": "^7.8.3",
    "@cloudflare/workers-types": "^1.0.6",
    "@storybook/addon-a11y": "^5.3.9",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-backgrounds": "^5.3.9",
    "@storybook/addon-info": "^5.3.9",
    "@storybook/addon-links": "^5.3.9",
    "@storybook/addons": "^5.3.9",
    "@storybook/preset-typescript": "^1.2.0",
    "@storybook/react": "^5.3.9",
    "@svgr/webpack": "^5.1.0",
    "@types/lodash": "4.14.149",
    "@types/node": "^13.5.0",
    "@types/reach__router": "^1.3.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/styled-components": "^4.4.2",
    "@types/tryghost__content-api": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.19.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-gatsby-package": "0.2.11",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "3",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-ghost": "1.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gatsby": "^2.19.5",
    "gatsby-cli": "^2.8.27",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-google-analytics": "^2.1.34",
    "gatsby-plugin-netlify": "^2.1.31",
    "gatsby-plugin-netlify-cms": "^4.1.37",
    "gatsby-plugin-postcss": "^2.1.19",
    "gatsby-plugin-react-css-modules": "^2.1.19",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sass": "^2.1.27",
    "gatsby-plugin-sharp": "^2.4.0",
    "gatsby-plugin-sitemap": "^2.2.26",
    "gatsby-remark-images": "^3.1.42",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-remark": "^2.6.48",
    "gatsby-transformer-sharp": "^2.3.13",
    "lerna": "^3.20.2",
    "node-sass": "^4.13.1",
    "npm-check-updates": "^4.0.1",
    "polished": "^3.4.2",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^4.7.1",
    "rimraf": "^3.0.0",
    "service-worker-mock": "^2.0.4",
    "styled-components": "^5.0.0",
    "typescript": "^3.7.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2",
    "wrangler": "^0.0.2"
  }
}
