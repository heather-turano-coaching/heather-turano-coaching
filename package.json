{
  "name": "@heather-turano-coaching/heather-turano-coaching",
  "version": "1.0.0",
  "description": "All applications and source code for https://livelifemindful.com",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "postinstall": "lerna run postinstall",
    "clean": "lerna clean --yes && lerna exec -- rimraf lib/ dist/",
    "bootstrap": "lerna bootstrap --hoist && rimraf **/@types/react-native",
    "bootstrap:ci": "lerna bootstrap --hoist",
    "version": "lerna version --yes",
    "reset": "yarn clean && yarn bootstrap",
    "release": "yarn build && lerna publish --yes",
    "compile": "lerna run compile",
    "check": "lerna exec -- npx npm-check-updates",
    "update": "lerna exec -- npx npm-check-updates -u",
    "test": "jest",
    "prestart": ". .env",
    "lint:fix": "eslint './packages/**/src/**/*.js' --fix",
    "build:core": "yarn build:design-system && yarn build:components && yarn build:domain",
    "build:components": "yarn build:hooks && lerna run build --scope=@heather-turano-coaching/components  --stream",
    "build:components:storybook": "yarn build:design-system && yarn build:hooks && lerna run build:storybook --scope=@heather-turano-coaching/components --stream",
    "build:design-system": "lerna run build --scope=@heather-turano-coaching/design-system --stream",
    "build:hooks": "lerna run build --scope=@heather-turano-coaching/hooks --stream",
    "build:website": "yarn build:core && lerna run build --scope=@heather-turano-coaching/website --stream",
    "build:mindful-movement": "yarn build:core && lerna run build --scope=@heather-turano-coaching/mindful-movement --stream",
    "build:packages": "yarn build:core && lerna run build --scope=@heather-turano-coaching/packages --stream",
    "build:api": "lerna run build --scope=@heather-turano-coaching/api --stream",
    "build:domain": "lerna run build --scope=@heather-turano-coaching/domain --stream",
    "publish:api:dev": "lerna run publish:dev --scope=@heather-turano-coaching/api --stream",
    "publish:api:prod": "lerna run publish:prod --scope=@heather-turano-coaching/api --stream",
    "publish:api:all": "yarn publish:api:dev && yarn publish:api:prod",
    "build:platform": "lerna run build --scope=@heather-turano-coaching/platform --stream",
    "start:design-system": "lerna run start --scope=@heather-turano-coaching/design-system --stream",
    "start:components": "yarn build:design-system && yarn build:hooks && cd ./packages/components && yarn start",
    "start:components:storybook": "yarn build:design-system && yarn build:hooks && cd ./packages/components && yarn buildDevUtils && yarn start:storybook",
    "start:website": "yarn build:core && cd packages/websites/website && yarn start",
    "start:mindful-movement": "yarn build:core && cd packages/websites/mindful-movement && yarn start",
    "start:packages": "yarn build:core && cd packages/websites/packages && yarn start",
    "start:api": "cd packages/api && yarn start",
    "start:domain": "cd packages/domain && yarn start",
    "start:hooks": "cd packages/hooks && yarn start",
    "start:platform": "lerna run dev --scope=@heather-turano-coaching/platform --stream",
    "preview:website": "yarn build:core && cd packages/websites/website && yarn serve",
    "preview:mindful-movement": "yarn build:core && cd packages/websites/mindful-movement && yarn serve",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate:platform": "lerna run generate --scope=@heather-turano-coaching/platform --stream",
    "serve:platform": "lerna run start --scope=@heather-turano-coaching/platform --stream",
    "create-migration": "cd packages/platform && yarn migrate -- ",
    "run-migrations": "cd packages/platform && yarn migrate:up",
    "solution:bootstrap": "source scripts/bootstrap.sh",
    "solution:reset": "source scripts/reset.sh"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:imaginedelements/heather-turano-coaching/live-life-mindful.git"
  },
  "keywords": [
    "live-life-mindful",
    "live",
    "life",
    "mindful",
    "heather-turano-coaching",
    "heather",
    "turano"
  ],
  "resolutions": {
    "graphql": "14.6.0"
  },
  "author": "Drew DeCarme (drew@imaginedelements.com)",
  "license": "ISC",
  "devDependencies": {
    "lerna": "3.20.2"
  },
  "dependencies": {}
}
