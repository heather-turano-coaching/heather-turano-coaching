{
  "name": "@heather-turano-coaching/heather-turano-coaching",
  "version": "1.0.0",
  "description": "All applications and source code for https://livelifemindful.com",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "postinstall": "npm rebuild sharp",
    "clean": "lerna clean --yes && lerna exec -- rimraf lib/ dist/",
    "bootstrap": "lerna bootstrap --hoist && rimraf **/@types/react-native",
    "bootstrap:ci": "lerna bootstrap --hoist",
    "version": "lerna version --yes",
    "reset": "yarn clean && yarn bootstrap",
    "release": "yarn build && lerna publish --yes",
    "compile": "lerna run compile",
    "check": "lerna exec -- npx npm-check-updates",
    "update": "lerna exec -- npx npm-check-updates -u",
    "format": "prettier --write \"packages/**/*.{js,jsx,json,md,ts,tsx}\"",
    "test": "jest",
    "prestart": ". .env",
    "lint:fix": "eslint './packages/**/src/**/*.js' --fix",
    "build:base": "yarn build:design-system && yarn build:components && yarn build:domain && yarn build:core",
    "build:core": "lerna run build --scope=@heather-turano-coaching/core",
    "build:components": "lerna run build --scope=@heather-turano-coaching/components  --stream",
    "build:components:storybook": "yarn build:base && lerna run build:storybook --scope=@heather-turano-coaching/components --stream",
    "build:design-system": "lerna run build --scope=@heather-turano-coaching/design-system --stream",
    "build:website": "yarn build:base && lerna run build --scope=@heather-turano-coaching/website --stream",
    "build:mindful-movement": "yarn build:base && lerna run build --scope=@heather-turano-coaching/mindful-movement --stream",
    "build:packages": "yarn build:base && lerna run build --scope=@heather-turano-coaching/packages --stream",
    "build:domain": "lerna run build --scope=@heather-turano-coaching/domain --stream",
    "build:api": "cd packages/api && yarn build",
    "start:design-system": "lerna run start --scope=@heather-turano-coaching/design-system --stream",
    "start:components": "yarn build:design-system && yarn build:core && cd ./packages/components && yarn start",
    "start:components:storybook": "yarn build:design-system && yarn build:core && cd ./packages/components && yarn buildDevUtils && yarn start:storybook",
    "start:website": "yarn build:base && cd packages/websites/website && yarn start",
    "start:mindful-movement": "yarn build:base && cd packages/websites/mindful-movement && yarn start",
    "start:packages": "yarn build:base && cd packages/websites/packages && yarn start",
    "start:domain": "cd packages/domain && yarn start",
    "start:api": "lerna run dev --scope=@heather-turano-coaching/api --stream",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate:api": "lerna run generate --scope=@heather-turano-coaching/api --stream",
    "serve:api": "lerna run start --scope=@heather-turano-coaching/api --stream",
    "create-migration": "cd packages/api && yarn migrate -- ",
    "run-migrations": "cd packages/api && yarn migrate:up",
    "solution:bootstrap": "source scripts/bootstrap.sh",
    "solution:reset": "source scripts/reset.sh"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:imaginedelements/heather-turano-coaching/live-life-mindful.git"
  },
  "keywords": [
    "live-life-mindful",
    "live",
    "life",
    "mindful",
    "heather-turano-coaching",
    "heather",
    "turano"
  ],
  "resolutions": {
    "graphql": "14.6.0"
  },
  "author": "Drew DeCarme (drew@imaginedelements.com)",
  "license": "ISC",
  "devDependencies": {
    "lerna": "3.22.1"
  },
  "dependencies": {}
}
