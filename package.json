{
  "name": "@heather-turano-coaching/heather-turano-coaching",
  "version": "1.0.0",
  "description": "All applications and source code for https://livelifemindful.com",
  "main": "index.js",
  "scripts": {
    "clean": "lerna clean --yes && lerna exec -- rimraf lib/ dist/",
    "bootstrap": "lerna bootstrap --force-local --hoist && rm -rf node_modules/@types/react-native",
    "bootstrap:ci": "lerna bootstrap",
    "version": "lerna version --yes",
    "reset": "npm run clean && npm run bootstrap",
    "release": "npm run build && lerna publish --yes",
    "compile": "lerna run compile",
    "check": "lerna exec -- ncu",
    "update": "npm run check -- -u && npm run bootstrap",
    "test": "jest",
    "lint:fix": "eslint './packages/**/src/**/*.js' --fix",
    "build:all": "npm run build:ds && npm run build:components && npm run build:blog && npm run build:serverless",
    "build:legacy": "lerna run build --scope=@heather-turano-coaching/legacy --stream",
    "build:website": "lerna run build --scope=@heather-turano-coaching/website --stream",
    "build:components": "lerna run build --scope=@heather-turano-coaching/components --stream",
    "build:components:storybook": "lerna run build:storybook --scope=@heather-turano-coaching/components --stream",
    "build:ds": "lerna run build --scope=@heather-turano-coaching/design-system --stream",
    "build:blog": "npm run build:ds && npm run build:components && lerna run build:static --scope=@live-life-mindful/blog",
    "build:serverless": "npm run build --scope=@heather-turano-coaching/serverless",
    "start:ds": "lerna run start --scope=@heather-turano-coaching/design-system --stream",
    "start:components": "npm run build:ds && cd ./packages/components && npm run start",
    "start:components:storybook": "npm run build:ds && cd ./packages/components && npm run start:storybook",
    "start:legacy": "lerna run dev --scope=@heather-turano-coaching/legacy",
    "start:website": "cd /Users/drewdecarme/git-imagined/heather-turano-coaching/live-life-mindful/packages/website && npm run dev --scope=@heather-turano-coaching/website",
    "start:blog": "cd packages/blog && npm run dev",
    "start:serverless": "cd packages/serverless && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:imaginedelements/heather-turano-coaching/live-life-mindful.git"
  },
  "keywords": [
    "live-life-mindful",
    "live",
    "life",
    "mindful",
    "heather-turano-coaching",
    "heather",
    "turano"
  ],
  "author": "Drew DeCarme (drew@imaginedelements.com)",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "lerna": "^3.20.2",
    "npm-check-updates": "^4.0.1",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5"
  }
}
