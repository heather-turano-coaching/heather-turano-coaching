{
  "extends": "@heather-turano-coaching/typescript/tsconfig.json",
  "include": ["src/**/*.ts", "src/**/*.tsx", "types/**/*.d.ts", ".eslintrc.js"],
  "compilerOptions": {
    "noEmit": false,
    "outDir": "./dist",
    "declaration": true,
    "declarationMap": true,
    "noUnusedLocals": true,
    "skipLibCheck": true
  }
}
