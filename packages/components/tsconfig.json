{
  "extends": "@heather-turano-coaching/typescript/tsconfig.json",
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "types/**/*.d.ts",
    ".eslintrc.js",
    "*.*.js"
  ],
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "isolatedModules": false,
    "target": "ES5",
    "module": "CommonJS"
  }
}
