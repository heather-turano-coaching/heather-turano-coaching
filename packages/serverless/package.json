{
  "name": "@heather-turano-coaching/serverless",
  "private": true,
  "version": "0.3.2",
  "description": "Cloudflare worker router converted to TS",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "dev": "NODE_ENV=development npm run build",
    "format": "prettier --write '**/*.{ts,js,css,json,md}'",
    "test:clean": "rimraf ./transpiled/src ./transpiled/test",
    "test": "npm run test:clean && npm run transpile && mocha --require source-map-support/register --recursive transpiled/test",
    "transpile": "tsc --project ./test",
    "start": "wrangler preview --watch",
    "publish:dev": "wrangler publish",
    "publish:prod": "wrangler publish --env=production"
  },
  "author": "Drew DeCarme <drewdecarme@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^1.0.6",
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "dotenv-webpack": "^1.7.0",
    "mocha": "^7.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "service-worker-mock": "^2.0.4",
    "source-map-loader": "^0.2.4",
    "source-map-support": "^0.5.16",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "wrangler": "^0.0.2"
  },
  "dependencies": {
    "query-string": "^6.10.1"
  }
}
