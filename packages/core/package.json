{
  "name": "@heather-turano-coaching/core",
  "version": "0.0.1",
  "description": "New version of the Heather Turano Coaching Component Library",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/heather-turano-coaching/heather-turano-coaching.git"
  },
  "scripts": {
    "build:theme": "rm -rf theme && tsc --project tsconfig.build-theme.json",
    "build:hooks": "rm -rf hooks && tsc --project tsconfig.build-hooks.json",
    "build:components": "rm -rf components && tsc --project tsconfig.build-components.json",
    "build": "yarn build:theme && yarn build:hooks && yarn build:components",
    "dev:theme": "yarn build:theme -w",
    "dev:hooks": "yarn build:theme -w",
    "dev:components": "yarn build:theme -w",
    "dev": "yarn dev:theme && yarn dev:hooks && yarn dev:components",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Drew DeCarme",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/heather-turano-coaching/heather-turano-coaching/issues"
  },
  "homepage": "https://github.com/heather-turano-coaching/heather-turano-coaching#readme",
  "dependencies": {
    "@heather-turano-coaching/components": "^0.5.0",
    "@heather-turano-coaching/design-system": "^0.2.5",
    "@material-ui/core": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "material-ui": "^0.20.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.2.0"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "material-ui": "^0.20.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@heather-turano-coaching/eslint-config": "^0.0.1",
    "@heather-turano-coaching/typescript": "^0.2.1",
    "@mdx-js/react": "^1.6.17",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@types/styled-components": "^5.1.3",
    "babel-loader": "^8.1.0",
    "react-is": "^16.13.1",
    "typescript": "^4.0.2"
  }
}
