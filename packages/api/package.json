{
  "name": "@heather-turano-coaching/api",
  "description": "The Heather Turano Coaching NodeJS GraphQL API for all HTC applications",
  "license": "Apache-2.0",
  "version": "0.0.0",
  "main": "src/app.js",
  "dependencies": {
    "@nexus/schema": "^0.17.0",
    "@prisma/client": "2.10.0",
    "@sendgrid/mail": "^7.4.0",
    "apollo-server-express": "^2.19.0",
    "bcrypt": "^5.0.0",
    "cookie": "^0.4.1",
    "dotenv": "^8.2.0",
    "eventbrite": "^1.2.2",
    "express": "^4.17.1",
    "graphql": "15.4.0",
    "graphql-scalars": "^1.4.1",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^3.1.16",
    "nexus-plugin-prisma": "^0.23.1",
    "pino": "^6.7.0",
    "pino-http": "^5.3.0",
    "stripe": "^8.121.0",
    "uuid": "^8.3.1"
  },
  "scripts": {
    "format": "npx prettier --write './**/*.{ts,md}'",
    "dev": "yarn prisma:generate && ts-node-dev --transpile-only ./src/app",
    "dev:typecheck": "tsc --noEmit --watch",
    "nexus:reflect": "ts-node src/schema",
    "build": "yarn nexus:reflect && tsc",
    "prisma:env": ". ../../.env",
    "prisma:create-migration": "yarn prisma:env && prisma migrate save",
    "prisma:migrate": "yarn prisma:env && prisma migrate up && yarn generate",
    "prisma:generate": "yarn prisma:env && prisma generate",
    "start": "ts-node ./src/app.ts",
    "seed": "ts-node prisma/seed.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@heather-turano-coaching/eslint-config": "^0.0.1",
    "@heather-turano-coaching/typescript": "^0.2.1",
    "@prisma/cli": "^2.11.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie": "^0.4.0",
    "@types/express": "^4.17.9",
    "@types/express-serve-static-core": "^4.17.13",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/pino-http": "^5.0.5",
    "dotenv-cli": "^4.0.0",
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "pg": "^8.5.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/heather-turano-coaching/heather-turano-coaching.git"
  },
  "author": "Drew DeCarme",
  "bugs": {
    "url": "https://github.com/heather-turano-coaching/heather-turano-coaching/issues"
  },
  "homepage": "https://github.com/heather-turano-coaching/heather-turano-coaching#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "./configs/nexus-test-environment.js"
  }
}
