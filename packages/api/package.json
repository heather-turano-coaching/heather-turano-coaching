{
  "name": "@heather-turano-coaching/api",
  "description": "The Heather Turano Coaching NodeJS GraphQL API for all HTC applications",
  "license": "Apache-2.0",
  "version": "0.0.0",
  "dependencies": {
    "@nexus/schema": "^0.16.0",
    "@prisma/client": "^2.9.0",
    "@sendgrid/mail": "^7.2.6",
    "apollo-server-express": "^2.18.2",
    "bcrypt": "^5.0.0",
    "cookie": "^0.4.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-scalars": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^3.1.12",
    "nexus-plugin-prisma": "^0.22.0",
    "pino": "^6.7.0",
    "pino-http": "^5.3.0",
    "stripe": "^8.107.0",
    "uuid": "^8.3.1"
  },
  "scripts": {
    "format": "npx prettier --write './**/*.{ts,md}'",
    "dev": "yarn prisma:generate && ts-node-dev --transpile-only ./src/app",
    "dev:typecheck": "tsc --noEmit --watch",
    "nexus:reflect": "ts-node --transpile-only src/schema",
    "build": "ts-node ./src/app",
    "prisma:create-migration": "prisma migrate save",
    "prisma:migrate": "prisma migrate up && yarn generate",
    "prisma:generate": "prisma generate",
    "seed": "ts-node prisma/seed.ts",
    "test": "jest"
  },
  "type": "module",
  "devDependencies": {
    "@heather-turano-coaching/eslint-config": "^0.0.1",
    "@heather-turano-coaching/typescript": "^0.2.1",
    "@prisma/cli": "^2.9.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie": "^0.4.0",
    "@types/express": "^4.17.8",
    "@types/express-serve-static-core": "^4.17.13",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/pino-http": "^5.0.5",
    "jest": "^26.5.0",
    "jest-environment-node": "^26.5.0",
    "pg": "^8.4.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.3"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/heather-turano-coaching/heather-turano-coaching.git"
  },
  "author": "Drew DeCarme",
  "bugs": {
    "url": "https://github.com/heather-turano-coaching/heather-turano-coaching/issues"
  },
  "homepage": "https://github.com/heather-turano-coaching/heather-turano-coaching#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "./configs/nexus-test-environment.js"
  }
